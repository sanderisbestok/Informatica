<?xml version="1.0" encoding="ISO-8859-1"?>
<SIMPLE NAME="Adder" PREFERREDNUMBERFORMAT="Hexadecimal" PREFERREDCYCLELENGTH="10">
    <PARAMETERS/>
    <FORMS>
        <POLYGON COLOR="#000000">
            <STROKE/>
            <POINT POSX="-20.0" POSY="-30.0"/>
            <POINT POSX="20.0" POSY="-10.0"/>
            <POINT POSX="20.0" POSY="10.0"/>
            <POINT POSX="-20.0" POSY="30.0"/>
            <POINT POSX="-20.0" POSY="10.0"/>
            <POINT POSX="-12.0" POSY="0.0"/>
            <POINT POSX="-20.0" POSY="-10.0"/>
            <POINT POSX="-20.0" POSY="-30.0"/>
            <POINT POSX="-20.0" POSY="-30.0"/>
        </POLYGON>
        <TEXT POSX="4.0" POSY="0.0" HALIGN="CENTER" VALIGN="CENTER"
            COLOR="#000000" FONT="Dialog,plain,12.0">Add </TEXT>
    </FORMS>
    <IO>
        <INPUT BITS="32" SIGNED="false" POSX="-20.0" POSY="-20.0" NAME="Arg0">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <INPUT BITS="32" SIGNED="false" POSX="-20.0" POSY="20.0" NAME="Arg1">
            <LABEL POSX="-10.0" POSY="0.0" HALIGN="RIGHT" VALIGN="CENTER"/>
        </INPUT>
        <OUTPUT BITS="32" SIGNED="false" POSX="20.0" POSY="0.0" NAME="Sum">
            <LABEL POSX="10.0" POSY="0.0" HALIGN="LEFT" VALIGN="CENTER"/>
        </OUTPUT>
    </IO>
    <MEMORY/>
    <INTERNALS DELAY="3">
        <ACTION EVENT="INPUT_CHANGE">		Sum = Arg0 + Arg1; </ACTION>
    </INTERNALS>
</SIMPLE>
